# platform :ios, '9.0'

source 'https://github.com/CocoaPods/Specs.git'

target 'IPAServer' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!
  inhibit_all_warnings!

  # Pods for IPAServer

  pod 'CommandLine'
  pod 'MUFoundation/MUPath'
  pod 'GCDWebServer'
  pod 'CocoaHTTPServer'
  pod 'libextobjc'
  pod 'AFNetworking'
  pod 'SSZipArchive'
  pod 'zipzap'

end
